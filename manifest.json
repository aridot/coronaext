{
    "name": "Corona Ext",
    "version": "1.0",
    "description": "Compare Google Locations with Corona map",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "declarativeContent",
        "tabs",
        "cookies",
        // "https://*/*",
        "https://israelcoronamap.co.il/",
        "https://www.google.com/"
    ],
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "manifest_version": 2
}